{
  "name": "easy-components",
  "private": false,
  "version": "0.0.0",

  "files": [
    "easy-components"
  ],
  "module": "./easy-components/easy-components.es.ts",
  "main": "./easy-components/easy-components.umd.ts",
  "exports": {
    ".": {
      "import": "./easy-components/easy-components.es.ts",
      "require": "./easy-components/easy-components.umd.ts"
    },
    "./dist/style.css": {
      "import": "./easy-components/style.css",
      "require": "./easy-components/style.css"
    }
  },
  "keywords": [
    "vue3",
    "image",
    "viewer",
    "vue",
    "vite",
    "preview",
    "picture",
    "photo"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "style": "stylelint \"src/**/*.(vue|scss|css)\" --fix"
  },
  "dependencies": {
    "@jsdawn/vue3-tinymce": "^1.1.7",
    "@rollup/plugin-typescript": "^8.3.3",
    "base-vite-ui": "^1.0.8",
    "default-passive-events": "^2.0.0",
    "element-plus": "^2.2.6",
    "eslint-config-myeslint": "^1.0.2",
    "sass": "^1.53.0",
    "stylelint": "13",
    "stylelint-config-myself": "^1.0.1",
    "tslib": "^2.4.0",
    "vite-plugin-dts": "^1.2.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.9.0",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^2.9.9",
    "vite-plugin-progress": "^0.0.3",
    "vite-plugin-restart": "^0.1.1",
    "vue-tsc": "^0.34.7"
  }
}
